! -------------------------------------------------
! Dracula-like dark theme
! -------------------------------------------------
*.foreground:   #f8f8f2
*.background:   #282a36
*.cursorColor:  #bd93f9

*.color0:  #21222c
*.color1:  #ff5555
*.color2:  #50fa7b
*.color3:  #f1fa8c
*.color4:  #bd93f9
*.color5:  #ff79c6
*.color6:  #8be9fd
*.color7:  #f8f8f2

*.color8:  #6272a4
*.color9:  #ff6e6e
*.color10: #69ff94
*.color11: #ffffa5
*.color12: #d6acff
*.color13: #ff92df
*.color14: #a4ffff
*.color15: #ffffff

! -------------------------------------------------
! xterm settings
! -------------------------------------------------
xterm*utf8: true
xterm*locale: true
xterm*renderFont: true

xterm*faceName: xft:Noto Sans Mono:style=Regular
xterm*faceSize: 10

xterm*scrollBar: false
xterm*scrollBarRight: false
xterm*rightScrollBar: false
xterm*toolBar: false

xterm*saveLines: 5000
xterm*bellIsUrgent: true
xterm*visualBell: false

